{
  "name": "promette-backend",
  "version": "1.0.0",
  "description": "Backend para promette con Express.js, TypeScript y MariaDB",
  "main": "dist/app.js",
  "scripts": {
    "start": "node dist/app.js",
    "dev": "nodemon src/app.ts",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rimraf dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:migrate:reset": "prisma migrate reset",
    "db:seed": "ts-node prisma/seed.ts",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "db:push": "prisma db push",
    "db:setup": "prisma migrate deploy && prisma db seed",
    "db:setup:dev": "prisma migrate dev && prisma db seed",
    "db:reset:force": "prisma migrate reset --force",
    "test:pool": "ts-node scripts/test-pool.ts",
    "pool:health": "curl -s http://localhost:3000/api/pool/health | json_pp",
    "pool:stats": "curl -s http://localhost:3000/api/pool/stats | json_pp",
    "diagnostico": "ts-node scripts/test-diagnostico.ts"
  },
  "keywords": [
    "promette",
    "express",
    "typescript",
    "mariadb",
    "jwt"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.13.0",
    "@types/archiver": "^6.0.3",
    "@types/bcrypt": "^6.0.0",
    "@types/handlebars": "^4.0.40",
    "@types/pdfkit": "^0.17.2",
    "@types/sharp": "^0.31.1",
    "archiver": "^7.0.1",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "express-rate-limit": "^8.0.1",
    "fs-extra": "^11.3.1",
    "handlebars": "^4.7.8",
    "helmet": "^7.1.0",
    "http-status-codes": "^2.3.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.10.1",
    "pdf-lib": "^1.17.1",
    "pdfkit": "^0.17.1",
    "prisma": "^6.13.0",
    "puppeteer": "^24.16.2",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "sharp": "^0.34.3",
    "tsconfig-paths": "^4.1.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "xlsx": "^0.18.5",
    "zod": "^4.0.15"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.5",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.20",
    "@types/fs-extra": "^11.0.4",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.13",
    "@types/node": "^20.19.9",
    "@types/nodemailer": "^6.4.17",
    "@types/uuid": "^9.0.8",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
